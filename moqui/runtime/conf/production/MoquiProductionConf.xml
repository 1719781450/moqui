<?xml version="1.0" encoding="UTF-8" ?>
<!-- No copyright or license for configuration file, details here are not considered a creative work. -->
<moqui-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/moqui-conf-1.0.xsd">

    <server-stats>
        <artifact-stats type="screen" track-bin="true" persist-bin="true" persist-hit="true"/>
        <artifact-stats type="screen-content" track-bin="true" persist-bin="true" persist-hit="true"/>
        <artifact-stats type="transition" track-bin="true" persist-bin="true" persist-hit="true"/>
        <artifact-stats type="service" track-bin="true" persist-bin="true" persist-hit="false"/>
        <artifact-stats type="entity" track-bin="false" persist-bin="false"/>
    </server-stats>

    <webapp-list>
        <webapp name="webroot" root-screen-location="component://webroot/screen/MoquiRoot.xml"/>
    </webapp-list>

    <screen-facade boundary-comments="false"/>

    <entity-facade crypt-pass="MoquiProductionDefaultPassword:CHANGEME"/>

    <repository-list>
        <repository name="main" location="http://localhost:8081/server" type="davex" workspace="production"/>
    </repository-list>
</moqui-conf>
