<entity-facade-xml type="seed">
    <!-- =========================== Data Source Type Data ==================== -->
    <EnumerationType description="Data Source Type" enumTypeId="DATA_SOURCE_TYPE"/>
    <Enumeration description="Purchased Data" enumId="DST_PURCHASED_DATA" enumTypeId="DATA_SOURCE_TYPE"/>
    <Enumeration description="Customer Data Entry" enumId="DST_CUSTOMER_ENTRY" enumTypeId="DATA_SOURCE_TYPE"/>
    <Enumeration description="Internal Data Entry (employees, etc)" enumId="DST_INTERNAL_ENTRY" enumTypeId="DATA_SOURCE_TYPE"/>
    <Enumeration description="Mailing List Sign-up" enumId="DST_MAILING_SIGNUP" enumTypeId="DATA_SOURCE_TYPE"/>

    <!-- =========================== Email Type Data ========================== -->
    <EmailServer emailServerId="SYSTEM" smtpRelayHost="" smtpUsername="" smtpPassword="" smtpPort="" smtpStartTls="" smtpSsl=""/>
    <!--
    <EmailTemplate emailTemplateId="PWD_RESET" description="" emailServerId="SYSTEM"
                   bodyScreenLocation="" xslfoAttachScreenLocation=""
                   fromAddresses="" ccAddresses="" bccAddresses=""
                   subject="" contentType="" sendPartial="Y"/>
    -->

    <EnumerationType description="Email Purpose" enumTypeId="EMAIL_PURPOSE"/>
    <Enumeration description="System" enumId="EMP_SYSTEM" enumTypeId="EMAIL_PURPOSE"/>
    <Enumeration description="Password Reset" enumId="EMP_PWD_RESET" parentEnumId="EMP_SYSTEM" enumTypeId="EMAIL_PURPOSE"/>
    <Enumeration description="Error Notice" enumId="EMP_ERROR_NOTICE" parentEnumId="EMP_SYSTEM" enumTypeId="EMAIL_PURPOSE"/>

    <StatusType description="Email Message Status" statusTypeId="EMAIL_STATUS"/>
    <StatusItem description="Draft" sequenceNum="1" statusId="ES_DRAFT" statusTypeId="EMAIL_STATUS"/>
    <StatusItem description="Ready" sequenceNum="2" statusId="ES_READY" statusTypeId="EMAIL_STATUS"/>
    <StatusItem description="Sent" sequenceNum="3" statusId="ES_SENT" statusTypeId="EMAIL_STATUS"/>
    <StatusItem description="Received" sequenceNum="4" statusId="ES_RECEIVED" statusTypeId="EMAIL_STATUS"/>
    <StatusItem description="Bounced" sequenceNum="8" statusId="ES_BOUNCED" statusTypeId="EMAIL_STATUS"/>
    <StatusItem description="Cancelled" sequenceNum="9" statusId="ES_CANCELLED" statusTypeId="EMAIL_STATUS"/>
    <StatusValidChange statusId="ES_DRAFT" statusIdTo="ES_READY" transitionName="Ready"/>
    <StatusValidChange statusId="ES_READY" statusIdTo="ES_SENT" transitionName="Send"/>
    <StatusValidChange statusId="ES_READY" statusIdTo="ES_CANCELLED" transitionName="Cancel"/>
    <StatusValidChange statusId="ES_SENT" statusIdTo="ES_RECEIVED" transitionName="Received"/>
    <StatusValidChange statusId="ES_SENT" statusIdTo="ES_BOUNCED" transitionName="Bounced"/>

    <!-- =========================== Status and Enum Type Data ================ -->
    <StatusType statusTypeId="_NA_" description="Not Applicable"/>
    <StatusItem statusId="_NA_" statusTypeId="_NA_" statusCode="NA" sequenceNum="0" description="Not Applicable"/>

    <EnumerationType enumTypeId="_NA_" description="Not Applicable"/>
    <Enumeration enumId="_NA_" enumTypeId="_NA_" enumCode="NA" sequenceNum="0" description="Not Applicable"/>

    <!-- =========================== Geo Type Data ============================ -->
    <!-- A placeholder for when there is no Geo -->
    <Geo geoId="_NA_" geoName="Not Applicable" geoCodeAlpha3="_NA" geoCodeAlpha2="_N"/>

    <EnumerationType description="Geo Type" enumTypeId="GEO_TYPE"/>

    <!-- General Geographic Groupings -->
    <Enumeration description="Group" enumId="GEOT_GROUP" enumTypeId="GEO_TYPE"/>
    <Enumeration description="Region" enumId="GEOT_REGION" enumTypeId="GEO_TYPE"/>
    <Enumeration description="Sales Region" enumId="GEOT_SALES_REGION" enumTypeId="GEO_TYPE"/>
    <Enumeration description="Service Region" enumId="GEOT_SERVICE_REGION" enumTypeId="GEO_TYPE"/>

    <!-- Legal/Governmental Geographic Groupings based on Juris-diction -->
    <Enumeration description="City" enumId="GEOT_CITY" enumTypeId="GEO_TYPE"/>
    <Enumeration description="State" enumId="GEOT_STATE" enumTypeId="GEO_TYPE"/>
    <Enumeration description="Postal Code" enumId="GEOT_POSTAL_CODE" enumTypeId="GEO_TYPE"/>
    <Enumeration description="Country" enumId="GEOT_COUNTRY" enumTypeId="GEO_TYPE"/>
    <Enumeration description="County" enumId="GEOT_COUNTY" enumTypeId="GEO_TYPE"/>
    <Enumeration description="County-City" enumId="GEOT_COUNTY_CITY" enumTypeId="GEO_TYPE"/>
    <Enumeration description="Municipality" enumId="GEOT_MUNICIPALITY" enumTypeId="GEO_TYPE"/>
    <Enumeration description="Province" enumId="GEOT_PROVINCE" enumTypeId="GEO_TYPE"/>
    <Enumeration description="Territory" enumId="GEOT_TERRITORY" enumTypeId="GEO_TYPE"/>

    <EnumerationType description="Geo Assoc Type" enumTypeId="GEO_ASSOC_TYPE"/>
    <Enumeration description="Geo Group Member" enumId="GAT_GROUP_MEMBER" enumTypeId="GEO_ASSOC_TYPE"/>
    <Enumeration description="For a region of a larger Geo, i.e. states, counties, provinces..." enumId="GAT_REGIONS" enumTypeId="GEO_ASSOC_TYPE"/>
    <Enumeration description="Administrative Main City of a County" enumId="GAT_COUNTY_SEAT" enumTypeId="GEO_ASSOC_TYPE"/>

    <!-- =========================== UOM Type Data ============================ -->
    <EnumerationType description="UOM Type" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Currency" enumId="UT_CURRENCY_MEASURE" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Data Size" enumId="UT_DATA_MEASURE" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Data Speed" enumId="UT_DATASPD_MEASURE" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Time/Frequency" enumId="UT_TIME_FREQ_MEASURE" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Length" enumId="UT_LENGTH_MEASURE" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Area" enumId="UT_AREA_MEASURE" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Liquid Volume" enumId="UT_VOLUME_LIQ_MEAS" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Dry Volume" enumId="UT_VOLUME_DRY_MEAS" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Weight" enumId="UT_WEIGHT_MEASURE" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Energy" enumId="UT_ENERGY_MEASURE" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Power" enumId="UT_POWER_MEASURE" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Temperature" enumId="UT_TEMP_MEASURE" enumTypeId="UOM_TYPE"/>
    <Enumeration description="Other" enumId="UT_OTHER_MEASURE" enumTypeId="UOM_TYPE"/>
</entity-facade-xml>
